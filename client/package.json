{
  "name": "vite-react-tailwind-eslint-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules",
    "full": "pnpm run test:coverage && pnpm run format-fix && pnpm run format && pnpm run build",
    "LINT": "------------------- LINT ----------------------",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "format": "biome format --verbose ./src",
    "format-fix": "biome format --verbose ./src --write",
    "format-prettier": "prettier . --check --config .prettierrc.mjs --ignore-path .prettierignore",
    "TEST": "------------------- TEST ----------------------",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc -p tsconfig.json",
    "STORYBOOK": "-------------- STORYBOOK -----------------",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "@react-google-maps/api": "^2.19.2",
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.3",
    "react-tooltip": "^5.26.0",
    "stylis-plugin-rtl": "^2.1.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@biomejs/biome": "1.5.3",
    "@chromatic-com/storybook": "^1.2.18",
    "@eslint/js": "^8.56.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/manager-api": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@testing-library/jest-dom": "^6.2.1",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "globals": "^13.24.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "storybook-addon-tag-badges": "^1.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "util": "^0.12.5",
    "vite": "^5.0.12"
  },
  "eslintConfig": {
    "extends": ["plugin:storybook/recommended"]
  }
}
